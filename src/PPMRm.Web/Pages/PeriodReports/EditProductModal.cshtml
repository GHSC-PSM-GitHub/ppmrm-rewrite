@page
@model PPMRm.Web.Pages.PeriodReports.EditProductModalModel
@using Volo.Abp.AspNetCore.Mvc.UI.Bootstrap.TagHelpers.Modal
@{
    Layout = null;
}
<abp-dynamic-form abp-model="Product" asp-page="/PeriodReports/EditProductModal">
    <abp-modal>
        <abp-modal-header title="Edit Product"></abp-modal-header>
        <abp-modal-body>
            <abp-form-content />
            <ul>
                <li>@Model.PeriodReportId</li>
                <li>@Model.ProgramId</li>
                <li>@Model.Id</li>
            </ul>
        </abp-modal-body>
        <abp-modal-footer buttons="@(AbpModalButtons.Cancel|AbpModalButtons.Save)"></abp-modal-footer>
    </abp-modal>
</abp-dynamic-form>