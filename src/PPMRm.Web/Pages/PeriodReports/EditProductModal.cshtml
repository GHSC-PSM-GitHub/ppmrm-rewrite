@page
@model PPMRm.Web.Pages.PeriodReports.EditProductModalModel
@using Volo.Abp.AspNetCore.Mvc.UI.Bootstrap.TagHelpers.Modal
@{
    Layout = null;
}
<abp-dynamic-form abp-model="Product" asp-page="/PeriodReports/EditProductModal">
    <abp-modal scrollable="true" size="Large">
        <abp-modal-header title="Edit Product - (@Model.PeriodReportId)"></abp-modal-header>
        <abp-modal-body>
            <abp-select asp-for="@Model.ProgramId" asp-items="@Model.Programs" disabled="true" />
            <abp-select asp-for="@Model.Id" asp-items="@Model.Products" disabled="true" />
            <abp-select asp-for="@Model.SOHLevels" asp-items="@Model.SOHLevelOptions" />
            <abp-form-content />
        </abp-modal-body>
        <abp-modal-footer buttons="@(AbpModalButtons.Cancel|AbpModalButtons.Save)"></abp-modal-footer>
    </abp-modal>
</abp-dynamic-form>