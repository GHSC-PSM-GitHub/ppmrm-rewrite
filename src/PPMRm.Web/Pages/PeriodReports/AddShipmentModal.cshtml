@page
@using Volo.Abp.AspNetCore.Mvc.UI.Bootstrap.TagHelpers.Modal
@model PPMRm.Web.Pages.PeriodReports.AddShipmentModalModel
@{
    Layout = null;
}

<abp-dynamic-form abp-model="Shipment" asp-page="/PeriodReports/AddShipmentModal">
    <abp-modal>
        <abp-modal-header title="Add Shipment"></abp-modal-header>
        <abp-modal-body>
            <abp-form-content />
            <ul>
                <li>@Model.PeriodReportId</li>
                <li>@Model.ProgramId</li>
                <li>@Model.Id</li>
            </ul>
        </abp-modal-body>
        <abp-modal-footer buttons="@(AbpModalButtons.Cancel|AbpModalButtons.Save)"></abp-modal-footer>
    </abp-modal>
</abp-dynamic-form>