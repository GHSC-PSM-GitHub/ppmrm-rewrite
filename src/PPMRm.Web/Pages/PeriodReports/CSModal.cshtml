@page
@using Volo.Abp.AspNetCore.Mvc.UI.Bootstrap.TagHelpers.Modal
@model PPMRm.Web.Pages.PeriodReports.CSModalModel
@{
    Layout = null;
}

<abp-dynamic-form abp-model="CSUpdates" asp-page="/PeriodReports/CSModal">
    <abp-modal scrollable="true" size="Large">
        <abp-modal-header title="CS Updates - @Model.Title"></abp-modal-header>
        <abp-modal-body>
            <abp-input asp-for="Id" />
            <abp-form-content abp-model="@Model.CSUpdates" />
        </abp-modal-body>
        <abp-modal-footer buttons="@(AbpModalButtons.Cancel|AbpModalButtons.Save)"></abp-modal-footer>
    </abp-modal>
</abp-dynamic-form>
